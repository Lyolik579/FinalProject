<h1>Browse Projects</h1>

<div class="row">
	<% @category.each do |category| %>
  <div class="col-sm-6 col-d-4">
    <div class="thumbnail">
    </div>
      <img src="/assets/bb.jpg" alt="BB">
      <div class="caption">
        <h3><%= category.name %></h3>
        <p>Description</p>
        <p>
        <%= link_to '<button class="btn btn-primary" role="button" type="button">View Category</button>'.html_safe, category_path(category) %>
    	</p>
    	<% end %>
    	</div>
  </div>
</div>